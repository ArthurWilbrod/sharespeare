<center>
	<div class="container">
		<br>
		<br>
    

      <h2 class="sign_up_text">Not registered yet ?</h2>
  

    
	  <h2>Sign Up</h2>
	  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    
          
	  	<div class="field form-group">
		    <label for="exampleInputEmail1">Email address</label>
		    <%= f.email_field :email, class: 'form-control', autofocus: true, autocomplete: "email" %>
	    </div>
      
	    <div class="field">
	      <%= f.label :password %><br />
	      <%= f.password_field :password, class: 'form-control', autofocus: true, autocomplete: "current-password" %>
	    </div>
      
	    <% if devise_mapping.rememberable? %>
	      <div class="field">
	        <%= f.check_box :remember_me %>
	        <%= f.label :remember_me %>
	        <br>
	        <br>
	      </div>
	    <% end %>
	    <div class="actions">
	      <%= f.submit "Go !", class:"btn btn-primary btn-round" %>
	      <br>
	      <br>
	    </div>
    <%end%>
	  <%= render "users/shared/links" %>
	</div>
</center>

